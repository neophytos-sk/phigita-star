contract {
    param -id msg -default "" -optional 1
    param -id msg2 -default "" -optional 1
    param -id msg3 -default "" -optional 1
    param -id upload_file -default "" -optional 1
}

master -src templates/blank-master.inc {
    ul {
        li { t "msg=@{msg}" }
        li { t "msg2=@{msg2}" }
        li { t "msg3=@{msg3}" }
    }
}
