    {values(//div[@id="main"]/dl[@class="article-photo-main-hor"]//img[@width]/@src)}
}
xpath_article_date {returndate(string(//div[@id="main"]//ins/@datetime),"%Y-%m-%dT%H:%M:%SZ")}
xpath_article_modified_time {}

